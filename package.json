{
  "name": "gyskate",
  "version": "0.0.2",
  "description": "The react website that is responsible for gyskate.com",
  "main": "src/app.js",
  "scripts": {
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "start-js": "react-scripts start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Turpster/gyskate.git"
  },
  "keywords": [
    "skateboarding",
    "portfolio-work",
    "gyskate"
  ],
  "author": "Turpster",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Turpster/gyskate/issues"
  },
  "homepage": "https://github.com/Turpster/gyskate#readme",
  "dependencies": {
    "proptypes": "^1.1.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-native": "^0.61.5",
    "react-native-local-resource": "^0.1.6",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.3.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
